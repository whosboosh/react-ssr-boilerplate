{
  "name": "moetorrent",
  "version": "1.0.0",
  "description": "A self hosted torrent client on the web, Powered by webtorrent!",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "cross-env NODE_ENV=development webpack --config webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=development webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "build:client:prod": "cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "build:server:prod": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build:prod": "npm run build:client:prod && npm run build:server:prod",
    "start": "node public/dist/server.min.js",
    "prod": "npm run build:prod && npm run start",
    "dev": "npm run build && npm run start"
  },
  "repository": "https://github.com/nathanial292/moetorrent",
  "author": "nathanial292",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "webtorrent": "^0.107.16"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "null-loader": "^3.0.0",
    "react-helmet": "^5.2.1",
    "react-loadable": "^5.5.0",
    "sass-loader": "^7.1.0",
    "serialize-javascript": "^1.7.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
